#include<bits/stdc++.h>
using namespace std;

int main(){
  int tuu;
  cin>>tuu;
  while(tuu--){
    string s;
    cin>>s;

    if(s.size() % 2){
      s.erase(s.begin()+ s.size()/2);
    }
    if(s!= std::string(s.size(),s[0]))
    {
      cout<<"yes"<<endl;
    }
    else{
      cout<<"no"<<endl;
    }
  }
}